<?xml version="1.0" ?>
<!--
App配置文件
-->
<configuration>
    <!--Runtime 许可-->
	<runtimekey license="ArcGIS Runtime许可信息"/>

    <!--系统工作空间目录-->
    <workspace path="/RuntimeViewer"/>

    <!--widget组件列表-->
    <widgetcontainer>
        <widget label="图层管理" classname="com.zrzyyzt.runtimeviewer.Widgets.LayerManagerWidget.LayerManagerWidget"
	             icon="widget_layermanager.png" select_icon="widget_layermanager_select2.png"/>
        <widget label="要素查询" classname="com.zrzyyzt.runtimeviewer.Widgets.QueryWidget.QueryWidget"
	            icon="widget_query.png" select_icon="widget_query_select2.png"/>
        <widget label="要素编辑" classname="com.zrzyyzt.runtimeviewer.Widgets.FeatureEditWidget.FeatureEditWidget"
                icon="widget_featureedit.png" select_icon="widget_featureedit_select2.png"/>
        <widget label="测距量算" classname="com.zrzyyzt.runtimeviewer.Widgets.CalculateWidget.CaculateWidget"
	            icon="widget_caculate.png" select_icon="widget_caculate_select2.png"/>
        <widget label="地图标绘" classname="com.zrzyyzt.runtimeviewer.Widgets.DrawWidget.DrawWidget1"
            icon="widget_caculate.png" select_icon="widget_caculate_select2.png"/>
        <widget label="书签管理" classname="com.zrzyyzt.runtimeviewer.Widgets.BookmarkWidget.BookmarkWidget"
                icon="widget_caculate.png" select_icon="widget_caculate_select2.png"/>
        <widget label="文档管理" classname="com.zrzyyzt.runtimeviewer.Widgets.DocManagerWidget.DocManagerWidget"
            icon="widget_caculate.png" select_icon="widget_caculate_select2.png" width="400"/>
        <widget label="测试" classname="com.zrzyyzt.runtimeviewer.Widgets.HelloWorldWidget"  width="600" />
    </widgetcontainer>
</configuration>